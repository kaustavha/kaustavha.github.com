                        li
                            a(href='https://github.com/kaustavha')
                                img(src='img/github.svg')
                                sub Github
                        li
                            a(href='https://www.linkedin.com/in/kaustav-haldar-32283462/')
                                img(src='img/linkedin.svg')
                                sub LinkedIn
                        li
                            a(href='mailto:hi@kaustav.me')
                                img(src='img/gmail.svg')
                                sub Contact
                    hr.grey
            section(id='faq')
                article
                    include faq
                    hr.grey

