doctype html
head
    title Kaustav Haldar - Personal site
    link(rel='stylesheet', href='kaustav-haldar-resume/lib/fonts.css')
    link(rel='stylesheet', href='kaustav-haldar-resume/lib/normalize.css')
    link(rel='stylesheet', href='kaustav-haldar-resume/lib/reset.css')
    link(rel='stylesheet', href='lib/index.css')
    script(src='kaustav-haldar-resume/lib/jq.min.js')
    body
        main
            section(id='content')
                article
                    include intro
                    hr.grey
                    ul
                        li
                            a(href="./kaustav-haldar-resume/")
